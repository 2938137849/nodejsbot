{
  "$schema": "schema/corpus.schema.json",
  "privateMSG": [
    {
      "name": ".d <表达式> <...>:骰子",
      "regexp": "^\\.d +([^ ]+)",
      "reply": "[FN:CQBotCOC.getDice]"
    }, {
      "name": ".dstat:查看全部简写",
      "regexp": "^\\.dstat$",
      "reply": "[FN:CQBotCOC.getDiceStat]"
    }, {
      "name": ".dset <key>[=<value>]:设置/删除简写",
      "regexp": "^\\.dset +(?<key>\\w[\\w\\d]+)(?:=(?<value>[+\\-*d0-9#]+))?",
      "reply": "[FN:CQBotCOC.getDiceSet]"
    }, {
      "name": "获取<type>列表",
      "regexp": "^获取(?<type>[^ ]+)$",
      "reply": "[FN:CQBotPlugin.getter]",
      "needAdmin": true,
      "forward": true
    }, {
      "name": "设置<type> <other>",
      "regexp": "^设置(?<type>[^ ]+) (?<other>.+)$",
      "reply": "[FN:CQBotPlugin.setter]",
      "needAdmin": true
    }, {
      "name": "<设置|删除>poke<other>",
      "regexp": "^(?<control>设置|删除)poke(?<other>.*)$",
      "reply": "[FN:CQBotPokeGroup.runPrivate]",
      "needAdmin": true,
      "isOpen": false
    }, {
      "name": "<私聊|群聊>列表<open>",
      "regexp": "^(?<type>私聊|群聊)列表(?<open>开|关)$",
      "reply": "[FN:CQBotPlugin.pluginList]",
      "needAdmin": true,
      "forward": true
    }, {
      "name": "私聊列表<open><nums>",
      "regexp": "^私聊列表(?<open>[开|关])(?<nums>\\s?\\d+)$",
      "reply": "[FN:CQBotPlugin.pluginStat]",
      "needAdmin": true
    }, {
      "name": "私聊列表<nums>",
      "regexp": "^私聊列表(?<nums>\\s?\\d+)$",
      "reply": "[FN:CQBotPlugin.pluginInfo]",
      "needAdmin": true
    }, {
      "name": "来点[<r18>]<key>色图",
      "regexp": "^[来來发發给給][张張个個幅点點份](?<r18>[Rr]18的?)?(?<keyword>.*?)?的?[色瑟][图圖]$",
      "reply": "[FN:CQBotLoLiSeTu.getSeTu]",
      "delMSG": 10
    }, {
      "name": "来点东方图",
      "regexp": "^来点东方图$",
      "reply": "[FN:CQBotTouHou.getTouHouPNG]",
      "isOpen": false
    }, {
      "name": "看_p站<pid>[-<p>]",
      "regexp": "^看{1,2}p站(?<pid>\\d+)(?:-(?<p>\\d+))?$",
      "reply": "[FN:CQBotPixiv.getPixiv]"
    }
  ],
  "groupMSG": [
    {
      "name": "为什么呢",
      "regexp": "^为什么呢$",
      "reply": "[CQ:reply]是啊，为什么呢，我也在寻找原因呢",
      "forward": false
    }, {
      "name": "有病吧",
      "regexp": "^有病吧$",
      "reply": "你治得好么?"
    }, {
      "name": ".搜图",
      "regexp": "^\\.搜图$",
      "reply": "[FN:CQBotSauceNAOGroup.getSauceNAO]"
    }, {
      "name": "复读",
      "regexp": "^.+$",
      "reply": "[FN:BotRepeat.getRepeat]"
    }, {
      "name": "@复读AI",
      "regexp": "^.*$",
      "reply": "[FN:CQBot.MemeAI]"
    }, {
      "name": "保留值",
      "regexp": "$^",
      "isOpen": false,
      "delMSG": 10,
      "forward": true,
      "needAdmin": true
    }
  ],
